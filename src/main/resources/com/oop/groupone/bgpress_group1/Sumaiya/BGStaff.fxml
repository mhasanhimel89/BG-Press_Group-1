            <Button mnemonicParsing="false" onAction="#updatejob" text="Update Job Progress" />
            <Button mnemonicParsing="false" onAction="#Reportmachineissues" text="Report Machine Issues" />
            <Button mnemonicParsing="false" onAction="#requestjob" text="Request New Job" />
            <Button mnemonicParsing="false" onAction="#submitwork" text="Submit Completed Work" />
            <Button mnemonicParsing="false" onAction="#feedback" text="Feedback" />
            <Button mnemonicParsing="false" onAction="#viewsalary" text="View Salary" />
            <Button mnemonicParsing="false" onAction="#signout" text="Signout" />
